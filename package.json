{
  "name": "vue-library-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:demo": "vue-cli-service build",
    "build:lib": "rollup -c && cp src/lib/index.d.ts dist/lib/index.d.ts",
    "build:lib-watch": "cp src/lib/index.d.ts dist/lib/index.d.ts && rollup -c --watch",
    "lib:version": "cd dist/lib && npm version",
    "lib:publish": "cd dist/lib && npm publish --access public",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@types/jest": "^24.0.11",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.7.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "glob-all": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-generate-package-json": "^3.1.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svgo": "^1.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-vue": "6.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^13.2.0",
    "tailwindcss": "^1.1.3",
    "tslib": "^1.10.0",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  }
}
